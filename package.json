{
  "name": "communityapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "android:release": "react-native run-android --variant=release",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "update:pods": "cd ios && pod install && cd ..",
    "postinstall": "yarn update:pods",
    "clear:cache": "rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && yarn cache clean && yarn install && yarn start --reset-cache",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "codegen": "graphql-codegen --config codegen.js",
    "setupdevlink": "yarn link react-graphql && yarn link react"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.13.2",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/bottom-tabs": "^5.11.3",
    "@react-navigation/native": "^5.9.0",
    "@react-navigation/stack": "^5.13.0",
    "dotenv": "^8.2.0",
    "graphql": "^15.5.0",
    "jwt-decode": "^3.1.2",
    "lodash.isequal": "^4.5.0",
    "react": "16.13.1",
    "react-native": "0.64.0",
    "react-native-auth0": "^2.7.0",
    "react-native-elements": "^3.1.0",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-image-crop-picker": "^0.36.0",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-localization": "^2.1.6",
    "react-native-localize": "^2.0.1",
    "react-native-maps": "^0.27.1",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.16.1",
    "react-native-touchable-scale": "^2.1.2",
    "react-native-vector-icons": "^7.1.0",
    "urql": "^2.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/typescript": "1.21.0",
    "@graphql-codegen/typescript-operations": "1.17.14",
    "@graphql-codegen/typescript-urql": "^2.0.3",
    "@graphql-codegen/urql-introspection": "1.0.0",
    "@react-native-community/eslint-config": "^1.1.0",
    "@types/jest": "^25.2.3",
    "@types/lodash.isequal": "^4.5.5",
    "@types/react": "^17.0.3",
    "@types/react-native": "^0.63.2",
    "@types/react-native-auth0": "^2.5.5",
    "@types/react-native-elements": "^0.18.0",
    "@types/react-native-vector-icons": "^6.4.6",
    "@types/react-navigation": "^3.4.0",
    "@types/react-test-renderer": "^16.9.2",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^6.5.1",
    "jest": "^25.1.0",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-test-renderer": "16.13.1",
    "typescript": "^3.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
